{
  "manifest_version": 3,
  "name": "LinkedIn Cleaner",
  "version": "1.0",
  "description": "Hides repetitive posts from your LinkedIn feed.",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.linkedin.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
 